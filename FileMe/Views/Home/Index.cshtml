@using FileMe.Models
@model HomeModel
@{
    ViewBag.Title = Model.Title;
}

@using (Html.BeginForm())
{
    <div>
        <h2>@Html.DisplayNameFor(m => m.Title)</h2>
    </div>
    <div>
        @Html.EditorFor(m => m.Title)
    </div>
    <div>
        <button type="submit">Отправить</button>
    </div>
    <div>
        @Model.Time.ToShortTimeString()
    </div>
}

<div>
    @Html.ActionLink("Создать пользователя", "Create", "Person")
</div>


@Html.ActionLink("Создать юзера", "Create", "User")